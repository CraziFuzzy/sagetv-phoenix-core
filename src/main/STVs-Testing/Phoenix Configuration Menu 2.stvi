               <Branch Name="else">
                <OptionsMenu Name="Instant Search Text Entry">
                 <IgnoreThemeProps>false</IgnoreThemeProps>
                 <Attribute Name="SearchString">
                  <Value>""</Value>
                 </Attribute>
                 <Action Name="&quot;Enter the search text:\n \n&quot;">
