                         </Action>
                        </Branch>
                        <Branch Name="else">
                         <Action ID="Phoenix Configuration Menu 2:8" Name="&quot;REM Finish setting up info for jump to search input screen.&quot;">
                          <Action Name="AddStaticContext(&quot;OnlineVideoSearchType&quot;, OnlineVideoItem )">
                           <Action Name="ref>>Menu:Online Video Search Results/Listener:Skip Fwd #2/Action:&quot;REM Jump to next category bar view style.&quot;/Conditional:Size( SubCategoryList ) > 1/Action:CurView = CurSubCategory/Action:CurViewIndex = FindElementIndex(SubCategoryList, CurView )/Action:CurViewIndex = CurViewIndex + 1/Action:SubCategoryItem = GetElement( SubCategoryList, CurViewIndex )/Conditional:IsSearch/Branch:else/Conditional:java_util_Properties_getProperty( gOnlineVideoListProps, SubCategoryItem + &quot;&amp;slash;IsSearch&quot;, &quot;false&quot; )/Branch:else/Action:&quot;REM Go ahead &amp; switch to this view.&quot;/Action:&quot;REM Selected a sub category, so change to it.&quot;/Conditional:SubCategoryItem != CurSubCategory/Conditional:IsSearch/Branch:else/Conditional:java_util_Properties_getProperty( gOnlineVideoListProps, SubCategoryItem + &quot;&amp;slash;IsSearch&quot;, &quot;false&quot; )/Branch:true/Action:AddStaticContext(&quot;OnlineVideoSearchType&quot;, SubCategoryItem )/Action:&quot;REM Jump to online video search input screen&quot;"/>
                          </Action>
                         </Action>
                        </Branch>
                       </Conditional>
                      </Action>
                     </Action>
                    </Action>
                   </Action>
                  </Action>
                 </Branch>
                </Conditional>
