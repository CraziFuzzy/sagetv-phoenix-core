                           <Action Name="TrackingCategory = java_util_Properties_getProperty( gOnlineVideoListProps, OnlineVideoItem + &quot;/TrackingCat&quot;, &quot;&quot; )">
                            <Conditional Name="Size( TrackingCategory ) > 0">
                             <Branch Name="true">
                              <Action Name="OnlineVideoItem = GetProperty( &quot;online_video/last_sub_category/&quot; + TrackingCategory, TrackingCategory )">
                               <Action Name="OnlineVideoSearchType ..." Ref="Phoenix Configuration Menu 2:10"/>
                              </Action>
                             </Branch>
                             <Branch Name="else">
                              <Action ID="Phoenix Configuration Menu 2:10" Name="OnlineVideoSearchType = OnlineVideoItem">
                               <Action Name="&quot;REM Selected Done, so do the search.&quot;">
                                <Action Name="&quot;REM Search the online video source for the text that was entered.&quot;">
                                 <Action Name="DebugLog(&quot;Searching &quot; + java_util_Properties_getProperty( gOnlineVideoTextProps, &quot;Source/&quot; + OnlineVideoType + &quot;/LongName&quot;, &quot;&quot; ) + &quot; (&quot; + OnlineVideoSearchType + &quot;) for:&quot; + SearchString)"/>
                                 <Action Name="ActionAfterRSSDownload = &quot;xJumpToResultsScreen&quot;">
                                  <Action Name="OnlineVideoItem = OnlineVideoSearchType"/>
                                 </Action>
                                 <Action Name="ref>>Menu:Online Video Search Results/Panel:NewsItems/Action:AllItems = sage_media_rss_RSSChannel_getItems(RSSChannel)/Action:RSSItemCount = Size(AllItems)/Action:&quot;REM Check to see if 'Previous' option is needed.&quot;/Conditional:ResultsIndex > 1/Branch:else/Action:&quot;REM Check to see if 'More' option is needed.&quot;/Conditional:RSSItemCount >= 20 &amp;&amp; (FeedContext != &quot;type=popular&quot;) &amp;&amp; (ResultsIndex != 0)/Branch:false/Action:&quot;REM Display the table or a message saying that no results were found.&quot;/Conditional:Size(AllItems) > 0/Branch:true/Action:AllItems/Table:NewsItems/TableComponent:RSSItem/Panel:Panel/Conditional:RSSItem/Branch:&quot;xMore&quot;/Item:More Search Results.../Action:&quot;REM Get the next page of results&quot;/Action:NewResultsIndex = ResultsIndex + 1/Action:ActionAfterRSSDownload = &quot;xRefreshScreen&quot;/Action:LastSubCat = CurSubCategory/Action:OnlineVideoItem = CurSubCategory/Action:NextResultsIndex = &quot;xForcedIndexValue&quot;/Conditional:IsSearch/Branch:true/Action:&quot;REM Perform the search.&quot;"/>
                                </Action>
                               </Action>
                              </Action>
                             </Branch>
                            </Conditional>
                           </Action>
                          </Action>
                         </Action>
                        </Action>
                       </Action>
                      </Branch>
                     </Conditional>
                    </Action>
                   </Action>
                  </Item>
                  <Item Name="Cancel">
                   <Attribute Name="DefaultFocus">
                    <Value>false</Value>
                   </Attribute>
                   <Action Name="CloseOptionsMenu()"/>
                  </Item>
                  <Theme Name="ref>>Theme:WideMultiButtonRowPanelTheme"/>
                 </Panel>
                 <Theme Name="ref>>Theme:TextEntryWithNTE_WideTheme"/>
                 <Hook Name="AfterMenuLoad">
                  <Conditional Name="UseSmartSearch">
                   <Branch Name="true">
                    <Action Name="Keystroke( KeyChar )"/>
                   </Branch>
                   <Branch Name="else">
                    <Conditional Name="java_lang_String_matches(KeyChar,&quot;[0-9]+&quot;)">
                     <Branch Name="true">
                      <Action Name="Number = KeyChar">
                       <Conditional Name="EnableNumericText &amp;&amp; (UseSmartSearch!=true)">
                        <Branch Name="else">
                         <Action Name="Keystroke(Number)" Ref="Phoenix Configuration Menu 2:11"/>
                        </Branch>
                        <Branch Name="true">
                         <Conditional Name="(LastNumber == null) || (Time() - LastNumberTime > GetProperty(&quot;ui/numeric_text_input_delay_time&quot;, &quot;1000&quot;)) || (Number != LastNumber)">
                          <Action Name="FirstInput = true">
                           <Action Name="NumericTextPos = 0">
                            <Conditional Name="ref>>Menu:Numeric Text Entry/Listener:Skip Fwd&amp;slash;Page Right - Accept current char/Action:&quot;REM Accept current char, if there is one&quot;/Conditional:LastNumber != null"/>
                           </Action>
                          </Action>
                         </Conditional>
                         <Conditional Name="!FirstInput &amp;&amp; false">
                          <Action Name="Keystroke(&quot;Backspace&quot;)"/>
                         </Conditional>
                         <Conditional Name="Number">
                          <Branch Name="0">
                           <Action Name="TextList = GetProperty(&quot;ui/numeric_text_input_&quot;+GetProperty(&quot;ui/translation_language_code&quot;,&quot;en&quot;)+&quot;_0&quot;+If(NTEState==&quot;xUpperCase&quot;,&quot;&quot;,&quot;_lower&quot;), &quot; 0&quot;)">
                            <Conditional Name="NumericTextPos >= Size..." Ref="Phoenix Configuration Menu 2:12"/>
                           </Action>
                          </Branch>
                          <Branch Name="1">
                           <Action Name="TextList = GetProperty(&quot;ui/numeric_text_input_&quot;+GetProperty(&quot;ui/translation_language_code&quot;,&quot;en&quot;)+&quot;_1&quot;+If(NTEState==&quot;xUpperCase&quot;,&quot;&quot;,&quot;_lower&quot;),&quot;1*?\&quot;\\/'`.,:;@$&amp;!-_()&quot;)">
                            <Conditional Name="NumericTextPos >= Size..." Ref="Phoenix Configuration Menu 2:12"/>
                           </Action>
                          </Branch>
                          <Branch Name="2">
                           <Action Name="TextList = GetProperty(&quot;ui/numeric_text_input_&quot;+GetProperty(&quot;ui/translation_language_code&quot;,&quot;en&quot;)+&quot;_2&quot;+If(NTEState==&quot;xUpperCase&quot;,&quot;&quot;,&quot;_lower&quot;), &quot;ABC2&quot;)">
                            <Conditional Name="NumericTextPos >= Size..." Ref="Phoenix Configuration Menu 2:12"/>
                           </Action>
                          </Branch>
                          <Branch Name="3">
                           <Action Name="TextList = GetProperty(&quot;ui/numeric_text_input_&quot;+GetProperty(&quot;ui/translation_language_code&quot;,&quot;en&quot;)+&quot;_3&quot;+If(NTEState==&quot;xUpperCase&quot;,&quot;&quot;,&quot;_lower&quot;), &quot;DEF3&quot;)">
                            <Conditional Name="NumericTextPos >= Size..." Ref="Phoenix Configuration Menu 2:12"/>
                           </Action>
                          </Branch>
                          <Branch Name="4">
                           <Action Name="TextList = GetProperty(&quot;ui/numeric_text_input_&quot;+GetProperty(&quot;ui/translation_language_code&quot;,&quot;en&quot;)+&quot;_4&quot;+If(NTEState==&quot;xUpperCase&quot;,&quot;&quot;,&quot;_lower&quot;), &quot;GHI4&quot;)">
                            <Conditional Name="NumericTextPos >= Size..." Ref="Phoenix Configuration Menu 2:12"/>
                           </Action>
                          </Branch>
                          <Branch Name="5">
                           <Action Name="TextList = GetProperty(&quot;ui/numeric_text_input_&quot;+GetProperty(&quot;ui/translation_language_code&quot;,&quot;en&quot;)+&quot;_5&quot;+If(NTEState==&quot;xUpperCase&quot;,&quot;&quot;,&quot;_lower&quot;), &quot;JKL5&quot;)">
                            <Conditional Name="NumericTextPos >= Size..." Ref="Phoenix Configuration Menu 2:12"/>
                           </Action>
                          </Branch>
                          <Branch Name="6">
                           <Action Name="TextList = GetProperty(&quot;ui/numeric_text_input_&quot;+GetProperty(&quot;ui/translation_language_code&quot;,&quot;en&quot;)+&quot;_6&quot;+If(NTEState==&quot;xUpperCase&quot;,&quot;&quot;,&quot;_lower&quot;), &quot;MNO6&quot;)">
                            <Conditional Name="NumericTextPos >= Size..." Ref="Phoenix Configuration Menu 2:12"/>
                           </Action>
                          </Branch>
                          <Branch Name="7">
                           <Action Name="TextList = GetProperty(&quot;ui/numeric_text_input_&quot;+GetProperty(&quot;ui/translation_language_code&quot;,&quot;en&quot;)+&quot;_7&quot;+If(NTEState==&quot;xUpperCase&quot;,&quot;&quot;,&quot;_lower&quot;), &quot;PQRS7&quot;)">
                            <Conditional Name="NumericTextPos >= Size..." Ref="Phoenix Configuration Menu 2:12"/>
                           </Action>
                          </Branch>
                          <Branch Name="8">
                           <Action Name="TextList = GetProperty(&quot;ui/numeric_text_input_&quot;+GetProperty(&quot;ui/translation_language_code&quot;,&quot;en&quot;)+&quot;_8&quot;+If(NTEState==&quot;xUpperCase&quot;,&quot;&quot;,&quot;_lower&quot;), &quot;TUV8&quot;)">
                            <Conditional Name="NumericTextPos >= Size..." Ref="Phoenix Configuration Menu 2:12"/>
                           </Action>
                          </Branch>
                          <Branch Name="9">
                           <Action Name="TextList = GetProperty(&quot;ui/numeric_text_input_&quot;+GetProperty(&quot;ui/translation_language_code&quot;,&quot;en&quot;)+&quot;_9&quot;+If(NTEState==&quot;xUpperCase&quot;,&quot;&quot;,&quot;_lower&quot;), &quot;WXYZ9&quot;)">
                            <Conditional ID="Phoenix Configuration Menu 2:12" Name="NumericTextPos >= Size(TextList)">
                             <Branch Name="true">
                              <Action Name="NumericTextPos = 0">
                               <Action Name="LastChar = Substring(T..." Ref="Phoenix Configuration Menu 2:13"/>
                              </Action>
                             </Branch>
                             <Branch Name="else">
                              <Action ID="Phoenix Configuration Menu 2:13" Name="LastChar = Substring(TextList,NumericTextPos,NumericTextPos+1)">
                               <Action Name="NumericTextPos = NumericTextPos + 1">
                                <Action Name="&quot;Keystroke(LastChar)&quot;">
                                 <Action Name="LastNumberTime = Time()">
                                  <Action Name="LastNumber = Number">
                                   <Conditional Name="UsingDelayedSearchInput">
                                    <Branch Name="true">
                                     <Action Name="ref>>Menu:Search - All Media Types/Hook:AfterMenuLoad/Action:&quot;REM If the menu is not being reloaded, check for initial search string, etc.&quot;/Conditional:Reloaded != true/Action:&quot;REM If received an initial search string via static context, set up the search.&quot;/Conditional:Size(InitialSearchString) > 0/Conditional:GetProperty(&quot;smart_search_enabled&quot;, true) &amp;&amp; GetProperty(&quot;enable_extras&quot;, false)/Branch:else/Conditional:GetProperty(&quot;instant_search&amp;slash;mode&quot;, &quot;xDialog&quot;)/Branch:&quot;xDialog&quot;/Action:SearchString = InitialSearchString/Action:&quot;REM Check last user input time before using input for delayed search&quot;"/>
                                     <Action Name="Refresh()"/>
