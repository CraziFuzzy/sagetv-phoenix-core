                  <Panel Name="ref>>Hook:RequestToExceedParentalRestrictions/Action:PCCode = &quot;&quot;/OptionsMenu:EnterPCCode/Conditional:DialogState/Branch:&quot;xAskForCode&quot;/Panel:PwdInputArea/Panel:TextEntryCursor"/>
                  <Shape Name="ref>>Hook:RequestToExceedParentalRestrictions/Action:PCCode = &quot;&quot;/OptionsMenu:EnterPCCode/Conditional:DialogState/Branch:&quot;xAskForCode&quot;/Panel:PwdInputArea/Shape:InputBorder"/>
